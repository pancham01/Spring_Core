<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
           http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <!-- Your Spring configuration goes here -->

	<context:annotation-config/>

	<bean id="address1" class="spring_core.dpInjectPro.Entity.Address">
		<property name="street" value="gandhi" />
		<property name="city" value="delhi"></property>
	</bean>
	<bean id="address2" class="spring_core.dpInjectPro.Entity.Address">
		<property name="street" value="gandhi_2" />
		<property name="city" value="delhi_2"></property>
	</bean>
	
	<!-- <bean id="employee1" class="spring_core.dpInjectPro.Entity.Employee">
		<constructor-arg name="name" value="Shukla" />
		<constructor-arg name="id" value="101"/>
	</bean> -->
	
	<bean id="employee" class="spring_core.dpInjectPro.Entity.Employee">
		<property name="name" value="Shukla" />
		<property name="id" value="101"/>
	</bean>
	
	<bean id="employee1" class="spring_core.dpInjectPro.Entity.Employee">
		<property name="name" value="Shukla2" />
		<property name="id" value="1012"/>
	</bean>
	
	
</beans>